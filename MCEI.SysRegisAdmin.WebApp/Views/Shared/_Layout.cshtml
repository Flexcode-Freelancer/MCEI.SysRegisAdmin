<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Mision Cristiana Elim Izalco</title>
    <link rel="icon" href="/assets/img/logos/Logo-Sin-Letras-White.png" type="image/x-icon">
    <link rel="stylesheet" href="~/MCEI.SysRegisAdmin.WebApp.styles.css" asp-append-version="true" />
    <link href="~/plugins/SweetAlert2/css/min.css" rel="stylesheet" />
    <link href="~/plugins/DataTables/css/min.css" rel="stylesheet" />
    <link href="~/plugins/DataTables/css/dataTable.css" rel="stylesheet" />
    <link href="~/plugins/select2/css/min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/assets/css/styles.min.css" />
        <link href="~/assets/css/custom.css" rel="stylesheet" />
    <script src="~/plugins/ChartJs/js/Chart.min.js"></script>
</head>
<body style="background-color:#f5f7ff">
    @if(User.Identity.IsAuthenticated)
    {
        <!--  Body Wrapper -->
        <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
             data-sidebar-position="fixed" data-header-position="fixed">
            <!-- Sidebar Start -->
            <aside class="left-sidebar">
                <!-- Sidebar scroll-->
                <div>
                    <div class="brand-logo d-flex align-items-center justify-content-between">
                        <a class="text-nowrap logo-img" asp-area="" asp-controller="Home" asp-action="Index">
                            <img src="/assets/img/logos/Logo-Elim-Black.png" width="180" alt="" />
                        </a>
                        <div class="close-btn d-xl-none d-block sidebartoggler cursor-pointer" id="sidebarCollapse">
                            <i class="ti ti-x fs-8"></i>
                        </div>
                    </div>
                    <!-- Sidebar navigation-->
                    <nav class="sidebar-nav scroll-sidebar" data-simplebar="">
                        <ul id="sidebarnav">
                            <!-- SECCION PRINCIPAL -->
                            <li class="nav-small-cap">
                                <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
                                <span class="hide-menu">Principal</span>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="Home" asp-action="Index" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-home"></i>
                                    </span>
                                    <span class="hide-menu">Inicio</span>
                                </a>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="Dashboard" asp-action="Index" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-layout-dashboard"></i>
                                    </span>
                                    <span class="hide-menu">Dashboard</span>
                                </a>
                            </li>
                            <!-- SECCION ADMINISTRACION -->
                            <li class="nav-small-cap">
                                <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
                                <span class="hide-menu">Administracion</span>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="Membership" asp-action="Index" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-building-church"></i>
                                    </span>
                                    <span class="hide-menu">Membresia</span>
                                </a>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="Server" asp-action="Index" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-server-cog"></i>
                                    </span>
                                    <span class="hide-menu">Servidores</span>
                                </a>
                            </li>
                            <!-- SECCION MANTENIMIENTO -->
                            <li class="nav-small-cap">
                                <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
                                <span class="hide-menu">Mantenimiento</span>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="ProfessionOrStudy" asp-action="Index" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-school"></i>
                                    </span>
                                    <span class="hide-menu">Profesion u Oficio</span>
                                </a>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="Privilege" asp-action="Index" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-address-book"></i>
                                    </span>
                                    <span class="hide-menu">Privilegios</span>
                                </a>
                            </li>
                            <!-- SECCION SEGURIDAD -->
                            <li class="nav-small-cap">
                                <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
                                <span class="hide-menu">Seguridad</span>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="Role" asp-action="Index" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-git-fork"></i>
                                    </span>
                                    <span class="hide-menu">Roles</span>
                                </a>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="User" asp-action="Index" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-users"></i>
                                    </span>
                                    <span class="hide-menu">Usuarios</span>
                                </a>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link" asp-area="" asp-controller="User" asp-action="ChangePassword" aria-expanded="false">
                                    <span>
                                        <i class="ti ti-refresh"></i>
                                    </span>
                                    <span class="hide-menu">Cambiar Contraseña</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <!-- End Sidebar navigation -->
                </div>
                <!-- End Sidebar scroll-->
            </aside>
            <!--  Sidebar End -->
            <!--  Main wrapper -->
            <div class="body-wrapper">
                <!--  Header Start -->
                <header class="app-header">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <ul class="navbar-nav">
                            <li class="nav-item d-block d-xl-none">
                                <a class="nav-link sidebartoggler nav-icon-hover" id="headerCollapse" href="javascript:void(0)">
                                    <i class="ti ti-menu-2"></i>
                                </a>
                            </li>
                        </ul>
                        <div class="navbar-collapse justify-content-end px-0" id="navbarNav">
                            <ul class="navbar-nav flex-row ms-auto align-items-center justify-content-end">
                                <li class="nav-item dropdown">
                                    <a class="nav-link nav-icon-hover" href="javascript:void(0)" id="drop2" data-bs-toggle="dropdown"
                                       aria-expanded="false">
                                        <img src="../assets/images/profile/user-1.jpg" alt="" width="35" height="35" class="rounded-circle">
                                    </a>
                                    <div class="dropdown-menu dropdown-menu-end dropdown-menu-animate-up" aria-labelledby="drop2">
                                        <div class="message-body">
                                            <a href="javascript:void(0)" class="d-flex align-items-center gap-2 dropdown-item">
                                                <i class="ti ti-user fs-6"></i>
                                                <p class="mb-0 fs-3">Mi Información</p>
                                            </a>
                                            <a asp-area="" asp-controller="User" asp-action="Logout" class="btn btn-outline-primary mx-3 mt-2 d-block">Cerrar Sesiòn</a>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </header>
                <!--  Header End -->

                <!-- Body Render-->
                <div class="container-fluid">
                    <main role="main">
                        @RenderBody()
                    </main>
                </div>

                <!-- Footer -->
                <div class="py-6 px-6 text-center">
                    <p class="mb-0 fs-4">&copy; 2024 Desarrollado Por Hector Garcia</p>
                </div>
            </div>
        </div>
    }
    else
    {
        <div>
            <main role="main">
                @RenderBody()
            </main>
        </div>
    }
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/plugins/SweetAlert2/js/min.js"></script>
    <script src="~/plugins/DataTables/js/min.js"></script>
    <script src="~/plugins/DataTables/js/dataTable.js"></script>
    <script src="~/plugins/Select2/js/min.js"></script>
    <script src="~/plugins/BarCode/js/min.js"></script>

    <script src="~/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/js/sidebarmenu.js"></script>
    <script src="~/assets/js/app.min.js"></script>
    <script src="~/assets/libs/apexcharts/dist/apexcharts.min.js"></script>
    <script src="~/assets/libs/simplebar/dist/simplebar.js"></script>
    <script src="~/assets/js/dashboard.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
