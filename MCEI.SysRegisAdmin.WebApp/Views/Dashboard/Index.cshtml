@{
    ViewData["Title"] = "Dashboard";
}

<canvas id="myDoughnutChart" width="400" height="400">Hola</canvas>

<script>
    const ctx = document.getElementById('myDoughnutChart');

    new Chart(ctx, {
        type: 'bar',
        data: {
            labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange'],
            datasets: [{
                label: '# of Votes',
                data: [12, 19, 3, 5, 2, 3],
                borderWidth: 1
            }]
        },
        options: {
            scales: {
                y: {
                    beginAtZero: true
                }
            }
        }
    });
</script>